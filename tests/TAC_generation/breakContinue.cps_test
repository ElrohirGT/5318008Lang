while (3 < 10) {
    do {
        break;
    } while (3 < 11);
    if (true) {
        break;
    }
    break;
}
---
GOTO GLOBAL_LOOP_0
SEC GLOBAL_LOOP_0_RETURN:

SEC GLOBAL_IF_2:
	IF -1 GOTO GLOBAL_IF_2_BODY
SEC GLOBAL_IF_2_BODY:
	GOTO GLOBAL_LOOP_0_RETURN
	GOTO GLOBAL_IF_2_RETURN
SEC GLOBAL_LOOP_0:
	SEC GLOBAL_LOOP_0_CONDITION:
	LT t1 3 10
	IF t1 GOTO GLOBAL_LOOP_0_BODY
	IF NOT t1 GOTO GLOBAL_LOOP_0_RETURN
	SEC GLOBAL_LOOP_0_UPDATE:
	GOTO GLOBAL_LOOP_0_CONDITION
SEC GLOBAL_LOOP_0_BODY:
	GOTO GLOBAL_LOOP_1
	SEC GLOBAL_LOOP_1_RETURN:
	GOTO GLOBAL_IF_2
	SEC GLOBAL_IF_2_RETURN:
	GOTO GLOBAL_LOOP_0_RETURN
	GOTO GLOBAL_LOOP_0_UPDATE
SEC GLOBAL_LOOP_1:
	GOTO GLOBAL_LOOP_1_BODY
	SEC GLOBAL_LOOP_1_CONDITION:
	LT t2 3 11
	IF t2 GOTO GLOBAL_LOOP_1_BODY
	IF NOT t2 GOTO GLOBAL_LOOP_1_RETURN
	SEC GLOBAL_LOOP_1_UPDATE:
	GOTO GLOBAL_LOOP_1_CONDITION
SEC GLOBAL_LOOP_1_BODY:
	GOTO GLOBAL_LOOP_1_RETURN
	GOTO GLOBAL_LOOP_1_UPDATE