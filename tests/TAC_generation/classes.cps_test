class Animal {
	let name: string;

	function setName(newName: string) {
		this.name = newName;
	}
}

let an = new Animal();
an.setName("Default Name");
---
ALLOC t7 4
// this
PARAM t7
// newAnimal()
CALL Animal_constructor 1
ALLOC t8 16
SBO t8 0 68
SBO t8 1 101
SBO t8 2 102
SBO t8 3 97
SBO t8 4 117
SBO t8 5 108
SBO t8 6 116
SBO t8 7 32
SBO t8 8 78
SBO t8 9 97
SBO t8 10 109
SBO t8 11 101
SBO t8 15 0
// this -> an
PARAM t7
// "Default Name"
PARAM t8
// an.setName("Default Name")
CALL Animal_setName 2
END

FUNC Animal_constructor:
	// this
	LOAD t1
	ADD t3 0 0
	SWO t1 t3 t2
	END
FUNC Animal_setName:
	// this
	LOAD t4
	// newName
	LOAD t5
	= t6 i32 0
	SWO t4 t6 t5
	END