let x = 5 + 9 / 2;
let y = 2 + x;

class Number {
    function getNumber() {
        return 5;
    }
}

let a = new Number();
let b = a.getNumber() + y;
---
= err u32 0
= addr u32 0
REFERENCE err zero_division_exception
REFERENCE addr err_pointer
SWO addr 0 err
EQ t2 2 0
IF t2 GOTO _EXCEPTION_HANDLER
DIV t1 9 2
ADD t3 5 t1
= t4 i32 t3
ADD t5 2 t4
= t6 i32 t5
ALLOC t9 0
// this
PARAM t9
// newNumber()
CALL Number_constructor 1
// this -> a
PARAM t9
// a.getNumber()
CALLRET t10 Number_getNumber 1
ADD t11 t10 t6
= t12 i32 t11
END

FUNC Number_constructor:
	// this
	LOAD t7
	END
FUNC Number_getNumber:
	// this
	LOAD t8
	RETURN 5
	END